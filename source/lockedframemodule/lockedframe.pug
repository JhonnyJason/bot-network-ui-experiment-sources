//- lockedframe structure
#lockedframe
    #big-lock-symbol: svg: use(href="#svg-lock-icon")

    h1 Your Key is Locked
    #locked-options-row 
        #locked-unlock-button.click-button.flashy-button 
            svg: use(href="#svg-unlock-icon")
            .label Unlock

        #locked-delete-button.click-button.flashy-button 
            svg: use(href="#svg-trash-icon")
            .label Delete
